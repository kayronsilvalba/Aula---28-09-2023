import java.util.Scanner;

public class For2{
    public static void main(){
        Scanner scanner = new Scanner(System.in);
        
        int x;
        int y;
        System.out.println("Informe um número: ");
        x = scanner.nextInt();
        System.out.println("Digite um número: ");
        y = scanner.nextInt();
        
        for(int i = x; i >= y; i--){
            System.out.println(i);
        }
        
        for(int i = x; i <= y; i++){
            System.out.println(i);
        }
    }
}    
